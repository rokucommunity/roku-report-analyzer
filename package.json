{
  "name": "roku-report-analyzer",
  "version": "0.3.9",
  "description": "Analyze roku crashlogs and leverage sourcemaps to translate pkg paths into original file paths",
  "license": "MIT",
  "author": "RokuCommunity",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "roku-report-analyzer": "dist/cli.js",
    "rra": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rokucommunity/roku-report-analyzer"
  },
  "scripts": {
    "preversion": "npm run build && npm run test && npm run lint",
    "build": "rimraf dist && tsc",
    "watch": "rimraf dist && tsc --watch",
    "lint": "eslint \"src/**\"",
    "format": "tsfmt -r",
    "test": "nyc mocha",
    "test:nocover": "mocha",
    "test:watch": "mocha --watch",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "files": [
    "dist/**/!(*.spec.*)*"
  ],
  "mocha": {
    "spec": "src/**/*.spec.ts",
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "fullTrace": true,
    "timeout": 20000,
    "watchExtensions": [
      "ts"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "!src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/chai": "^4.3.0",
    "@types/chai-subset": "^1.3.3",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.18",
    "@types/sinon": "^10.0.11",
    "@types/source-map": "^0.5.7",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "adm-zip": "^0.5.9",
    "chai": "^4.3.6",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.9.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "typescript-formatter": "^7.2.2"
  },
  "dependencies": {
    "@rokucommunity/logger": "^0.3.3",
    "brighterscript": "^0.65.10",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.0.0",
    "glob-all": "^3.2.1",
    "source-map": "^0.7.3",
    "yargs": "^17.3.1"
  }
}
